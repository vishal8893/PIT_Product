<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Your Page Title</title>
    <style>
        .container {
            margin: 0 auto;
            /* This centers the container horizontally */
            max-width: 800px;
            /* Adjust this width as needed */
            padding: 20px;
            /* Add padding to create space on the sides */
        }

        header {
            background-color: #007BFF;
            /* Set the background color for the header */
            color: #FFFFFF;
            /* Set the text color for the header */
            text-align: center;
            padding: 10px 0;
            /* Add padding to the header */
        }

        /* Footer Styles */
        footer {
            background-color: #007BFF;
            /* Set the background color for the footer */
            color: #FFFFFF;
            /* Set the text color for the footer */
            text-align: center;
            padding: 10px 0;
            /* Add padding to the footer */
        }

        table {
            width: 100%;
        }

        table th,
        table td {
            word-wrap: break-word;
            max-width: 150px; /* Adjust the max-width as needed */
        }
    </style>
    <!-- Add your CSS and other meta tags as needed -->
</head>

<body>
    <header>
        <span class="title" style="text-align: center; display: block; width: 100%;">Commodity Details</span>
        <br />
    </header>
    <div class="container">
        <div>
            <div>
                <h2>Commodity Details:</h2>
                <table border="1" style="margin-top: 20px;">
                    <tr>
                        <th>CLIENT NAME</th>
                        <th>COMTRADE PARTICIPANT NAME</th>
                        <th>COMTRADE ID</th>
                        <th>Proofs/Authorization</th>                
                    </tr>
                    <% FinishData.Commodity.forEach(function(data) { %>
                        <tr>
                            <td>
                                <%= data.CLIENTNAME %>
                            </td>
                            <td>
                                <%= data.DP_BROKER_NAME %>
                            </td>
                            <td>
                                <%= data.DP_ACCOUNT_NO %>
                            </td>
                            <td>
                                <% if (data.UPLOAD_PATH) { %>
                                    <% const filename = data.UPLOAD_PATH.split('/').pop(); %>
                                    <%= filename %>
                                <% } else if (data.PROVIDE_DEMAT) { %>
                                        <% if (FinishData.entity.includes(data.DP_BROKER_NAME)) { %>
                                            Authorize Compliance
                                        <% } else { %>
                                            Will Provide Later
                                        <% } %>                                
                                <% } %>
                            </td>                    
                        </tr>
                    <% }); %>
                </table>
            </div>
        </div>
        <div>
            <div>
                <h2>Other than exchnage:</h2>
                <table border="1" style="margin-top: 20px;">
                    <tr>
                        <th>CLIENT NAME</th>
                        <th>Have you done any trade/s in Commodities(other than Future Exchange)</th>
                        <th>Proofs</th>                
                    </tr>
                    <% FinishData.phsical.forEach(function(data) { %>
                        <tr>
                            <td>
                                <%= data.CLIENT_ID %>
                            </td>
                            <td>
                                <%= data.IS_ANY_TRADE %>
                            </td>
                            <td>
                                <% if (data.UPLOAD_PATH) { %>
                                    <% const filename = data.UPLOAD_PATH.split('/').pop(); %>
                                    <%= filename %>
                                <% } %>
                            </td>                    
                        </tr>
                    <% }); %>
                </table>
            </div>
        </div>
    </div>
    <footer>
        <p>Newel Technologies Pvt</p>
        <!-- You can add any content you want in the footer section -->
    </footer>
</body>

</html>
